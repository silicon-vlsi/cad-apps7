#%Module1.0#####################################################################
##
## env modulefile
##
## modulefiles/env.  Generated by hand.
##
proc ModulesHelp { } {
        global version

        puts stderr "\tThis module sets up environment variable for XFAB's XT018 project" 
        puts stderr "\n\tVersion $version\n"
}

module-whatis	"Sets up the environment for XFAB XT018 (0.18um CMOS 200v SOI) for Cadence 6.18"

# for Tcl script use only
set     version      "1.0"

conflict trainings 


#CDSLICPORT is set in /CAD/apps7/etc/silicon.csh (linked to /etc/profile.d)
#IP address of cdslicsrv.vlsi.silicon.ac.in is set in /etc/hosts
set 	license_server     	cdslicsrv.vlsi.silicon.ac.in
setenv	CDS_LICENSE_FILE  	$env(CDSLICPORT)@${license_server}
#
setenv 	X_DIR			/PDK/xfab/XKIT
# T_DIR is obsolete for XENV but the .rsf file in LVS still contains that.
setenv 	T_DIR		 	$env(X_DIR)	
setenv  TRAINING		XT018
setenv  YEAR			22_1
#SIPROJ_DIR is set in /etc/profile.d/silicon.csh
setenv  WORKPATH		/home/nfs2/trainings/$env(TRAINING)/$env(YEAR)/work/$env(USER)

# Cadence tools
# IC616: All Virtuoso tools MMSIM:analog(SPectre..)
# INCISIVE: digital(ncsim, etc), ASSURA/PVS: verification
# NOTE: ASSURA license is included in PVS
# EXT: QRC extraction

module load tools/IC/618
module load tools/MMSIM/151		
#module load tools/INCISIVE/152		
module load tools/ASSURA/41-618		
module load tools/PVS/19-14-ISR4
module load tools/EXT/19-13		
module load tools/calibre/2021-4-33-16

# other changes to search path
prepend-path	PATH	$env(X_DIR)/x_all/cadence/xenv
#append-path     PATH    $env(SHARED_HOME)/digital/tools/bin

# aliases
#set-alias "qsub"   {qsub -wd $pwd -b yes $1}
#NOTE You cannot use quotes to pass environment variables. This is the only way
set-alias cadstart	{cd \$WORKPATH; virtuoso &}
set-alias pdproj	{pushd \$WORKPATH}
set-alias cdproj	{cd \$WORKPATH}
#set-alias cdmod		{cd $env(X_DIR)/xc06/cadence/v4_1/spectre/v4_1_2/xc06/models}
#set-alias dcelldoc	{/usr/bin/firefox /home/NIS/projects/XC06M3-18/A0/docs/DigIOESDdocs/xc06-D_CELLS-data_book/index.html &}
 
