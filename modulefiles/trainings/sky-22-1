#%Module1.0#####################################################################
##
## env modulefile
##
## modulefiles/env.  Generated by hand.
##
proc ModulesHelp { } {
        global version

        puts stderr "\tThis module sets up environment variable for SKY130 using OpenSource tools" 
        puts stderr "\n\tVersion $version\n"
}

module-whatis	"Sets up the environment for SKY130 for OpenSource TOols"

# for Tcl script use only
set     version      "1.0"

conflict trainings


setenv  TRAINING		SKY130
setenv  YEAR			22_1
#SIPROJ_DIR is set in /etc/profile.d/silicon.csh
setenv  WORKPATH		/home/nfs2/trainings/$env(TRAINING)/$env(YEAR)/work/$env(USER)

module load tools/opensrc/ngspice
module load tools/opensrc/magic
module load tools/opensrc/netgen

set-alias cdproj	{cd \$WORKPATH}
