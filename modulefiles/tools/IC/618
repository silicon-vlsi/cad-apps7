#%Module1.0#####################################################################
##
## IC (Virtuoso layout and schematics tool) modulefile
##
#https://support.cadence.com/apex/ArticleAttachmentPortal?id=a1O3w000009y0fFEAQ&pageName=ArticleContent
## IC/616.  Generated by hand.
##
proc ModulesHelp { } {
        global version

        puts stderr "\tThis module sets up Cadence Virtuoso Env."
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "Sets up the Cadence Virtuoso Environment"

# for Tcl script use only
set     version      "618"

conflict IC

#Set the license file server
# $CDSLICPORT is set in the globla csh file /etc/profile.d/silicon.csh
#IP address of cdslicsrv is set in /etc/hosts
set license_file            $env(CDSLICPORT)@cdslicsrv
setenv LM_LICENSE_FILE $license_file
setenv CDS_LIC_FILE    $license_file

#setenv PROJPATH                /Q/proj/$env(PROJECT)
setenv CDS_Netlisting_Mode      Analog
setenv CDS_AUTO_64BIT		ALL
setenv CDS_AHDLCMI_ENABLE 	YES
setenv EDITOR			vim
setenv LANG 			en_US
#
setenv CDS_INST_DIR             /CAD/cadence/IC618
setenv ICCHOME                  $env(CDS_INST_DIR)
setenv CDSHOME                  $env(CDS_INST_DIR)
setenv CDSDOC                  $env(CDS_INST_DIR)/doc
setenv LD_LIBRARY_PATH		$env(CDS_INST_DIR)/tools/lib
setenv OA_HOME 			$env(CDS_INST_DIR)/oa_v22.60.063
#

## Add the paths
prepend-path PATH           $env(CDS_INST_DIR)/tools.lnx86/bin:$env(CDS_INST_DIR)/tools.lnx86/dfII/bin:$env(CDS_INST_DIR)/tools/dfII/bin:$env(CDS_INST_DIR)/tools/bin:$env(CDS_INST_DIR)/bin

## The followings were copied from work but not setup yet.
#append-path MANPATH         $env(CDS_INST_DIR)/man
#append-path MANPATH         $env(CDS_INST_DIR)/share/man

#setenv WORKPATH            $env(PROJPATH)/work/$env(USER)
#set-alias icfb              "cd $env(WORKPATH); virtuoso"
#set-alias virtuoso         { virtuoso }
#set-alias lmstart	"lmgrd -c /CAD/license/License_3_28_2017.dat"
