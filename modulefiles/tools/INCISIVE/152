#%Module1.0#####################################################################
##
## Cadence Incisive modulefile
##
## INCISIVE/152.  Generated by hand.
##
proc ModulesHelp { } {
        global version

        puts stderr "\tThis module sets up Cadence Incisive."
        puts stderr "\n\tVersion $version\n"
}

module-whatis	"Sets up the Cadence Environment"

# for Tcl script use only
set     version      "152"

conflict INCISIVE


#setenv LICENSE_QUEUEING yes
set cds_home /CAD/apps7/INCISIVE152

#CDSLICPORT is set in /CAD/apps7/etc/silicon.csh (linked to /etc/profile.d)
#IP address of LICENSE-SRV is set in /etc/hosts
setenv CDS_LIC_FILE $env(CDSLICPORT)@{srv02.vlsi.silicon.ac.in}

#setenv CDSHOME $cds_home
setenv IUSHOME $cds_home
setenv INCISIVEHOME $cds_home
setenv LANG en_US
setenv CDS_Netlisting_Mode Analog
setenv SPECTRE_DEFAULTS "+l %C:r.out"
#setenv CDS_LOAD_ENV  CSF
#set-alias nclicense "lmstat -c $env(CDS_LIC_FILE) -f Xcelium_Single_Core"

prepend-path LD_LIBRARY_PATH $cds_home/tools/lib:/usr/lib:/lib
prepend-path PATH            $cds_home/tools/bin:$cds_home/tools/dfII/bin:$cds_home/tools.lnx86/bin:$cds_home/share/bin:$cds_home/bin

append-path MANPATH          $cds_home/man
append-path MANPATH          $cds_home/share/man
