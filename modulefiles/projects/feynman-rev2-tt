#%Module1.0#####################################################################
##
## env modulefile
##
## modulefiles/env.  Generated by hand.
##
proc ModulesHelp { } {
        global version

        puts stderr "\tThis module sets up standard environment for doing Analog Simulation using Tanner 2020"
        puts stderr "\n\tVersion $version\n"
}

module-whatis	"Sets up the environment for XFAB XT018 (0.18um CMOS) for Tanner "

# for Tcl script use only
set     version      "1.0"

conflict projects


#
setenv 	X_DIR			/PDK/tech_svn/xfab/XKIT
# T_DIR is obsolete for XENV but the .rsf file in LVS still contains that.
setenv 	T_DIR			$env(X_DIR)	
setenv  PROJECT			FEYNMAN	
setenv  REV			REV2
#SIPROJ_DIR is set in /etc/profile.d/silicon.csh
setenv  WORKPATH		$env(SIPROJ_DIR)/$env(PROJECT)/$env(REV)/work/tt-$env(USER)
setenv	MODPATH			$env(WORKPATH)/.xkit/setup/xt018/synopsys/hspice/lp5mos
setenv	TDOC			/CAD/mentor/tannereda/tanner-2020-3u3/tanner-2020-3-rhel6-64/tannereda/tools/docs	
setenv  XDOC			$env(SIPROJ_DIR)/$env(PROJECT)/$env(REV)/docs

# Load environment module for Tanner tools
module load tools/tanner/2020-3u3
module load tools/calibre/2021-4-33-16
## Calibre DRC/LVS rulset directory
#This will autmaatically reflect the current PDK usage status
setenv	XFAB_CALIBRE_RUNSET	$env(WORKPATH)/.xkit/setup/xt018/mentor/calibre
# For MIMANT checks include the following env variable in DRCOptions->Include
#setenv	XFAB_CALIBRE_MIMANT_RUNSET	$env(WORKPATH)/.xkit/setup/xx018/mentor/calibre_MIMANT

# other changes to search path
#prepend-path	PATH	$env(X_DIR)/x_all/cadence/xenv
#append-path     PATH    $env(SHARED_HOME)/digital/tools/bin

# aliases
#NOTE You cannot use quotes to pass environment variables. This is the only way
set-alias xkit		{tclsh \$X_DIR/x_all/tanner/xenv/xkit.tbc}
set-alias sedstart	{cd \$WORKPATH; sedit -n lib.defs &}
set-alias ledstart	{cd \$WORKPATH; ledit -n lib.defs &}
set-alias pdproj	{pushd \$WORKPATH}
set-alias cdproj	{cd \$WORKPATH}
set-alias cdmod		{cd \$MODPATH}
set-alias cdtdoc	{cd \$TDOC} 
set-alias cdxdoc	{cd \$XDOC} 
#set-alias dcelldoc	{/usr/bin/firefox /home/NIS/projects/XC06M3-18/A0/docs/DigIOESDdocs/xc06-D_CELLS-data_book/index.html &}
#set-alias iocelldoc	{/usr/bin/firefox /home/NIS/projects/XC06M3-18/A0/docs/DigIOESDdocs/xc06-IO_CELLS-data_book/index.html &}
#set-alias iocellfdoc	{/usr/bin/firefox /home/NIS/projects/XC06M3-18/A0/docs/DigIOESDdocs/xc06-IO_CELLS_F-data_book/index.html &}
