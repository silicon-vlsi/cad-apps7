#%Module1.0#####################################################################
##
## env modulefile
##
## modulefiles/env.  Generated by hand.
##
proc ModulesHelp { } {
        global version

        puts stderr "\tThis module sets up standard environment for doing Analog Simulation using Tanner 2018v3"
        puts stderr "\n\tVersion $version\n"
}

module-whatis	"Sets up the environment for XFAB XDH10 (1um CMOS) for Tanner "

# for Tcl script use only
set     version      "1.0"

conflict projects


#
#setenv 	X_DIR			/PDK/xfab/XKIT
setenv 	X_DIR			/PDK/tech_svn/xfab/XKIT
# T_DIR is obsolete for XENV but the .rsf file in LVS still contains that.
setenv 	T_DIR			$env(X_DIR)	
setenv  PROJECT			TESLA	
setenv  REV			REV1
#SIPROJ_DIR is set in /etc/profile.d/silicon.csh
setenv  WORKPATH		$env(SIPROJ_DIR)/$env(PROJECT)/$env(REV)/work/tt-$env(USER)
setenv	MODPATH			$env(X_DIR)/xdh10/synopsys/v4_0/hspice/v4_0_13/sitris
setenv	TDOC			/CAD/mentor/tannereda/tanner/docs			
setenv  XDOC			$env(SIPROJ_DIR)/$env(PROJECT)/$env(REV)/docs

# Load environment module for Tanner tools
#module load tools/tanner/2020-1u4
#module load tools/calibre/2020-2-14-12
module load tools/tanner/2020-3u3
module load tools/calibre/2021-4-33-16
## Calibre DRC/LVS rulset directory
#This will autmaatically reflect the current PDK usage status
setenv	XFAB_CALIBRE_RUNSET	$env(WORKPATH)/.xkit/setup/xdh10/siemens/calibre

# aliases
#NOTE You cannot use quotes to pass environment variables. This is the only way
set-alias xkit		{tclsh \$X_DIR/x_all/siemens/xenv/TannerTools/xkit.tbc}
set-alias sedstart	{cd \$WORKPATH; sedit -n lib.defs &}
set-alias ledstart	{cd \$WORKPATH; ledit -n lib.defs &}
set-alias pdproj	{pushd \$WORKPATH}
set-alias cdproj	{cd \$WORKPATH}
set-alias cdmod		{cd \$MODPATH}
set-alias cdtdoc	{cd \$TDOC} 
set-alias cdxdoc	{cd \$XDOC} 
